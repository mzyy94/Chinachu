{
  "/": {
    "script" : "recordings",
    "methods": ["get"],
    "types"  : ["json"]
  },
  
  "/:id": {
    "script" : "recording-program",
    "methods": ["get", "delete"],
    "types"  : ["json"]
  },
  
  "/:id/preview": {
    "script" : "recording-program-preview",
    "methods": ["get"],
    "types"  : ["png", "jpg", "txt"]
  },
  
  "/:id/watch": {
    "script" : "recording-program-watch",
    "methods": ["get"],
    "types"  : ["txt", "m3u8", "xspf", "m2ts", "f4v", "flv", "webm", "asf"]
  }
}